{
    "name": "@byungi/event-emitter",
    "description": "A tiny event emitter that works old browser, and supports typescript.",
    "version": "1.0.5",
    "author": "skt-t1-byungi <tiniwz@gmail.com>",
    "repository": "https://github.com/skt-t1-byungi/event-emitter.git",
    "main": "dist/index.umd.js",
    "module": "dist/index.esm.js",
    "unpkg": "dist/index.umd.js",
    "types": "dist/index.d.ts",
    "engines": {
        "node": ">= 6"
    },
    "files": [
        "dist/"
    ],
    "keywords": [
        "event",
        "emitter",
        "ie"
    ],
    "license": "MIT",
    "scripts": {
        "build": "rm -rf dist && bili src/index.ts --format esm,umd --module-name EventEmitter --bundle-node-modules",
        "test": "ava",
        "prepublishOnly": "npm run test && npm run build"
    },
    "dependencies": {
        "@skt-t1-byungi/array-filter": "^0.2.0",
        "@skt-t1-byungi/array-find-index": "^0.2.0",
        "@skt-t1-byungi/array-for-each": "^0.2.0"
    },
    "devDependencies": {
        "ava": "^1.4.1",
        "bili": "^4.7.3",
        "rollup-plugin-typescript2": "^0.21.0",
        "ts-node": "^8.1.0",
        "tslint": "^5.15.0",
        "tslint-config-byungi": "^0.1.1",
        "typescript": "^3.4.3"
    },
    "publishConfig": {
        "access": "public"
    },
    "ava": {
        "compileEnhancements": false,
        "extensions": [
            "ts"
        ],
        "require": [
            "ts-node/register"
        ]
    }
}
