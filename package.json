    "version": "1.0.0",
    "author": "skt-t1-byungi <tiniwz@gmail.com>",
    "repository": "https://github.com/skt-t1-byungi/rsup-progress.git",
    "main": "dist/index.umd.js",
    "module": "dist/index.mjs",
    "unpkg": "dist/index.umd.min.js",
    "types": "index.d.ts",
    "engines": {
        "node": ">= 6"
    },
    "files": [
