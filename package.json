        "bili": "^4.2.5",
        "rollup-plugin-typescript2": "^0.19.2",
        "ts-node": "^8.0.2",
        "tslint": "^5.12.1",
        "tslint-config-byungi": "^0.1.0",
        "typescript": "^3.3.3"
    },
    "publishConfig": {
        "access": "public"
    },
    "ava": {
